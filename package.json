{
  "name": "cultination-app-server",
  "version": "0.1.0",
  "description": "Cultination Mobile App Server is a component of a larger system designed to provide services for aquaculture farmers. This API server provides a way to interact with the system programmatically.",
  "scripts": {
    "start": "uvicorn app.main:app --reload",
    "start:prod": "gunicorn --workers 1 --worker-class uvicorn.workers.UvicornWorker --bind :9000 app.main:app",
    "live": "gunicorn -w 4 -k uvicorn.workers.UvicornWorker -b 0.0.0.0:9000 app.main:app --reload &",
    "live2": "nohup gunicorn -w 2 -k uvicorn.workers.UvicornWorker -b 0.0.0.0:9000 app.main:app --reload > app.log 2>&1 &",
    "test": "pytest"
  },
  "keywords": [
    "FastAPI",
    "Python"
  ],
  "author": "Yahya Aqrom",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/SensyncxPandawa/Cultination-Backend-V2-Trading/issues"
  },
  "homepage": "https://github.com/SensyncxPandawa/Cultination-Backend-V2-Trading#readme"
}
